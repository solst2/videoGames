<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>videoGameBean</managed-bean-name>
  <managed-bean-class>ch.hevs.managedbeans.VideoGameBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>welcomeVideoGames.xhtml</display-name>
  <from-view-id>/welcomeVideoGames.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>overviewForm</from-outcome>
   <to-view-id>/overviewForm.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>menuAdmin</from-outcome>
   <to-view-id>/menuAdmin.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>overviewForm.xhtml</display-name>
  <from-view-id>/overviewForm.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>rentGame</from-outcome>
   <to-view-id>/rentGame.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>giveBackGame</from-outcome>
   <to-view-id>/giveBackGame.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>#{videoGameBean.showGameDetails}</from-outcome>
   <to-view-id>/detailsGame.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>rentGame.xhtml</display-name>
  <from-view-id>/rentGame.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>#{videoGameBean.rentTheGame}</from-outcome>
   <to-view-id>/showClientResult.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>#{videoGameBean.showGameDetails}</from-outcome>
   <to-view-id>/detailsGame.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>giveBackGame.xhtml</display-name>
  <from-view-id>/giveBackGame.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>#{videoGameBean.giveGameBack}</from-outcome>
   <to-view-id>/showClientResult.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>detailsGame.xhtml</display-name>
  <from-view-id>/detailsGame.xhtml</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <display-name>menuAdmin.xhtml</display-name>
  <from-view-id>/menuAdmin.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>manageGameOverview</from-outcome>
   <to-view-id>/manageGameOverview.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>#{videoGameBean.managedPersonsView}</from-outcome>
   <to-view-id>/managePersonOverview.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>manageCategoryOverview</from-outcome>
   <to-view-id>/manageCategoryOverview.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>showAdminResult.xhtml</display-name>
  <from-view-id>/showAdminResult.xhtml</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <display-name>showClientResult.xhtml</display-name>
  <from-view-id>/showClientResult.xhtml</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <display-name>manageGame.xhtml</display-name>
  <from-view-id>/manageGame.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>#{videoGameBean.modifyGame}</from-outcome>
   <to-view-id>/showAdminResult.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>#{videoGameBean.deleteGame}</from-outcome>
   <to-view-id>/showAdminResult.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>addGame.xhtml</display-name>
  <from-view-id>/addGame.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>#{videoGameBean.addGame}</from-outcome>
   <to-view-id>/showAdminResult.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/manageGameOverview.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>manageGame</from-outcome>
   <to-view-id>/manageGame.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addGame</from-outcome>
   <to-view-id>/addGame.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>managePersonOverview.xhtml</display-name>
  <from-view-id>/managePersonOverview.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>managePerson</from-outcome>
   <to-view-id>/managePerson.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addPerson</from-outcome>
   <to-view-id>/addPerson.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>managePerson.xhtml</display-name>
  <from-view-id>/managePerson.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>#{videoGameBean.modifyPerson}</from-outcome>
   <to-view-id>/showAdminResult.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>#{videoGameBean.deletePerson}</from-outcome>
   <to-view-id>/showAdminResult.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>addPerson.xhtml</display-name>
  <from-view-id>/addPerson.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>#{videoGameBean.addPerson}</from-outcome>
   <to-view-id>/showAdminResult.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>addCategory.xhtml</display-name>
  <from-view-id>/addCategory.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>#{videoGameBean.addCategory}</from-outcome>
   <to-view-id>/showAdminResult.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>manageCategory.xhtml</display-name>
  <from-view-id>/manageCategory.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>#{videoGameBean.modifyCategory}</from-outcome>
   <to-view-id>/showAdminResult.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>#{videoGameBean.deleteCategory}</from-outcome>
   <to-view-id>/showAdminResult.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>manageCategoryOverview.xhtml</display-name>
  <from-view-id>/manageCategoryOverview.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>manageCategory</from-outcome>
   <to-view-id>/manageCategory.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addCategory</from-outcome>
   <to-view-id>/addCategory.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
